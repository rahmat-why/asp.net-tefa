@model ASP.NET_TEFA.Models.MsVehicle

@{
    ViewData["Title"] = "Delete";
}

<b>Apakah anda yakin untuk menghapus kendaraan ini?</b>
<div>
    <h4>MsVehicle</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-6">
            Tipe Kendaraan:
        </dt>
        <dd class = "col-sm-6">
            <b>@Html.DisplayFor(model => model.Type)</b>
        </dd>
        <dt class = "col-sm-6">
            Klasifikasi:
        </dt>
        <dd class = "col-sm-6">
            <b>@Html.DisplayFor(model => model.Classify)</b>
        </dd>
        <dt class = "col-sm-6">
            No. Polisi:
        </dt>
        <dd class = "col-sm-6">
            <b>@Html.DisplayFor(model => model.PoliceNumber)</b>
        </dd>
        <dt class = "col-sm-6">
            Warna:
        </dt>
        <dd class = "col-sm-6">
            <b>@Html.DisplayFor(model => model.Color)</b>
        </dd>
        <dt class = "col-sm-6">
            Tahun:
        </dt>
        <dd class = "col-sm-6">
            <b>@Html.DisplayFor(model => model.Year)</b>
        </dd>
        <dt class = "col-sm-6">
            Nama Pemilik:
        </dt>
        <dd class = "col-sm-6">
            <b>@Html.DisplayFor(model => model.VehicleOwner)</b>
        </dd>
        <dt class = "col-sm-6">
            Nomor Rangka:
        </dt>
        <dd class = "col-sm-6">
            <b>@Html.DisplayFor(model => model.ChassisNumber)</b>
        </dd>
        <dt class = "col-sm-6">
            Nomor Mesin:
        </dt>
        <dd class = "col-sm-6">
            <b>@Html.DisplayFor(model => model.MachineNumber)</b>
        </dd>
    </dl>
    <!-- Loading Spinner -->
    <div id="loadingSpinner" class="text-primary">
        Loading...
    </div>
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdVehicle" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Back to List</a>
    </form>
</div>
